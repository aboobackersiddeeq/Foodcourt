	<script>
		function changeCount(proid, userid) {


			$.ajax({
				url: '/add-to-cart',
				data: {
					proid: proid,
					userid: userid

				},
				method: "post",
				success: (response) => {
					if (response.count) {
						// Swal.fire({
						// 	icon: 'success',
						// 	title: 'Your work has been saved',
						// 	showConfirmButton: false,
						// 	timer: 1500
						// })
						location.href = "/viewcart"

						document.getElementById('count').innerHTML = response.count
					} else {

						location.href = "/login"

					}

				}
			})
		} 
    function deleted(Id) {
        Swal.fire({
            title: 'Are you sure?',
            text: "You won't be able to revert this!",
            icon: 'warning',
            showCancelButton: true,
            confirmButtonColor: '#3085d6',
            cancelButtonColor: '#d33',
            confirmButtonText: 'Yes, delete it!'
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    url: '/delet-wishlist',
                    data: {
                        id: Id,

                    },
                    method: "get",
                    success: (response) => {
                        if (response.status) {
                            document.getElementById(Id).remove()
                            Swal.fire(
                                'Deleted!',
                                'Your file has been deleted.',
                                'success'
                            )

                            console.log(response);
                             
                            
                        } else {
                            document.getElementById(Id).remove()
                            Swal.fire(
                                'Deleted!',
                                'Your file has been deleted.',
                                'success'
                            )
                             
                            location.reload()
                        } 
                    }
                })

            }
        })

    }
 
	</script>

